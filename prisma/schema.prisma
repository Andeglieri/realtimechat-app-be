generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  email     String   @unique
  username  String   @unique(map: "user_userName_key")
  password  String
}
